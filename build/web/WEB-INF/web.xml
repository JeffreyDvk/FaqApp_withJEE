<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>connect</servlet-name>
        <servlet-class>MServlet.connect</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deconnection</servlet-name>
        <servlet-class>MServlet.deconnection</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>inscription</servlet-name>
        <servlet-class>MServlet.inscription</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>postQuestion</servlet-name>
        <servlet-class>MServlet.postQuestion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>suppression</servlet-name>
        <servlet-class>MServlet.suppression</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>response</servlet-name>
        <servlet-class>MServlet.response</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addFAQ</servlet-name>
        <servlet-class>MServlet.addFAQ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>outFAQ</servlet-name>
        <servlet-class>MServlet.outFAQ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>anonymous</servlet-name>
        <servlet-class>MServlet.anonymous</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>connect</servlet-name>
        <url-pattern>/connect</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deconnection</servlet-name>
        <url-pattern>/deconnection</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>inscription</servlet-name>
        <url-pattern>/inscription</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>postQuestion</servlet-name>
        <url-pattern>/postQuestion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>suppression</servlet-name>
        <url-pattern>/suppression</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>response</servlet-name>
        <url-pattern>/response</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addFAQ</servlet-name>
        <url-pattern>/addFAQ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>outFAQ</servlet-name>
        <url-pattern>/outFAQ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>anonymous</servlet-name>
        <url-pattern>/anonymous</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <jsp-config>
        <jsp-property-group>
            <url-pattern>*.jsp</url-pattern>
            <include-prelude>/WEB-INF/taglibs.jsp</include-prelude>
        </jsp-property-group>
    </jsp-config>
</web-app>
